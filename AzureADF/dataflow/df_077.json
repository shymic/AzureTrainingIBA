{
	"name": "df_077",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sourceCSV",
						"type": "DatasetReference"
					},
					"name": "V37S2P1"
				},
				{
					"dataset": {
						"referenceName": "ds_sourceCSV",
						"type": "DatasetReference"
					},
					"name": "V37S4P2"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_sinkTable",
						"type": "DatasetReference"
					},
					"name": "V37S5",
					"description": "IPPF_WBS_POC_PLAN_LOAD"
				}
			],
			"transformations": [
				{
					"name": "V37S5P1Derive",
					"description": "derive - DS V37S0"
				},
				{
					"name": "V37S5P1",
					"description": "select  - DS V37S0"
				},
				{
					"name": "V37S4P1",
					"description": "select  - DS V37S2"
				},
				{
					"name": "V37S0P1Join",
					"description": "join - DS V37S4"
				},
				{
					"name": "V37S0P1",
					"description": "select  - DS V37S4"
				}
			],
			"scriptLines": [
				"source(output(",
				"          WBS_ID as string,",
				"          COST_ELEM_CD as string,",
				"          WBS_ELEM_OC_AMT as decimal(15,2),",
				"          MONTH_NUM as integer,",
				"          YEAR_NUM as integer,",
				"          WBS_ELEM_CC_AMT as decimal(15,2),",
				"          OBJ_CRNCY_CD as string,",
				"          CNTRL_CRNCY_CD as string,",
				"          WBS_LVL_NUM as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     maxColumns: 200) ~> V37S2P1",
				"source(output(",
				"          COST_ELEM_CD as string,",
				"          VAL_CAT_CD as string,",
				"          REV_IND as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     maxColumns: 200) ~> V37S4P2",
				"V37S0P1 derive(OBJ_AMT = case(trim(REV_IND)  == 'REVENUE',-1*OBJ_AMT,OBJ_AMT),",
				"          CONTRL_AMT = case(trim(REV_IND)  == 'REVENUE',-1*CONTRL_AMT,CONTRL_AMT),",
				"          TXN_AMT = case(trim(REV_IND)  == 'REVENUE',-1*OBJ_AMT,OBJ_AMT),",
				"          TXN_CRNCY_CD = OBJ_CRNCY_CD) ~> V37S5P1Derive",
				"V37S5P1Derive select(mapColumn(",
				"          WBS_ID,",
				"          COST_ELEM_CD,",
				"          VAL_CAT_CD,",
				"          POC_PLAN_FINL_MO,",
				"          POC_PLAN_FINL_YR,",
				"          OBJ_CRNCY_CD,",
				"          OBJ_AMT,",
				"          CONTRL_AMT,",
				"          CONTRL_CRNCY_CD,",
				"          REV_IND,",
				"          TXN_AMT,",
				"          TXN_CRNCY_CD",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> V37S5P1",
				"V37S2P1 select(mapColumn(",
				"          WBS_ID,",
				"          COST_ELEM_CD,",
				"          POC_PLAN_FINL_MO = MONTH_NUM,",
				"          POC_PLAN_FINL_YR = YEAR_NUM,",
				"          OBJ_CRNCY_CD,",
				"          OBJ_AMT = WBS_ELEM_OC_AMT,",
				"          CONTRL_AMT = WBS_ELEM_CC_AMT,",
				"          CONTRL_CRNCY_CD = CNTRL_CRNCY_CD",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> V37S4P1",
				"V37S4P1, V37S4P2 join(V37S4P1@COST_ELEM_CD == V37S4P2@COST_ELEM_CD,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> V37S0P1Join",
				"V37S0P1Join select(mapColumn(",
				"          WBS_ID,",
				"          COST_ELEM_CD = V37S4P1@COST_ELEM_CD,",
				"          VAL_CAT_CD,",
				"          POC_PLAN_FINL_MO,",
				"          POC_PLAN_FINL_YR,",
				"          OBJ_CRNCY_CD,",
				"          OBJ_AMT,",
				"          CONTRL_AMT,",
				"          CONTRL_CRNCY_CD,",
				"          REV_IND",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> V37S0P1",
				"V37S5P1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     preSQLs:['DELETE FROM STAG.WBS_POC_PLAN_LOAD'],",
				"     postSQLs:['DELETE FROM STAG.WBS_POC_PLAN_LOAD WHERE CAST(WBS_ID AS CHAR(1))!=\\'C\\''],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> V37S5"
			]
		}
	}
}