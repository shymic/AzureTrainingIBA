{
	"name": "df_009",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_sourceCSV",
						"type": "DatasetReference"
					},
					"name": "V164S6P1"
				},
				{
					"dataset": {
						"referenceName": "ds_sourceCSV",
						"type": "DatasetReference"
					},
					"name": "V164S8P2"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_sinkTable",
						"type": "DatasetReference"
					},
					"name": "V164S14",
					"description": "DB2_Connector_67"
				}
			],
			"transformations": [
				{
					"name": "V164S8P1Derive",
					"description": "derive - DS V164S6"
				},
				{
					"name": "V164S8P1",
					"description": "select  - DS V164S6"
				},
				{
					"name": "V164S14P2Join",
					"description": "join - DS V164S8"
				},
				{
					"name": "V164S14P2",
					"description": "select  - DS V164S8"
				}
			],
			"scriptLines": [
				"parameters{",
				"     p_IPPF_RULES_SCHEME as string",
				"}",
				"source(output(",
				"          WBS_ID as string,",
				"          COST_ELEM_CD as string,",
				"          WBS_ELEM_OC_AMT as decimal(15,2),",
				"          MONTH_NUM as integer,",
				"          YEAR_NUM as integer,",
				"          WBS_ELEM_CC_AMT as decimal(15,2),",
				"          OBJ_CRNCY_CD as string,",
				"          CNTRL_CRNCY_CD as string,",
				"          WBS_LVL_NUM as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     maxColumns: 200) ~> V164S6P1",
				"source(output(",
				"          COST_ELEM_CD as string,",
				"          VAL_CAT_CD as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     maxColumns: 200) ~> V164S8P2",
				"V164S6P1 derive(SOURCE_IND = 'BH',",
				"          PLAN_AMT = 0,",
				"          PLAN_CRNCY_CD = '',",
				"          TXN_AMT = 0,",
				"          TXN_CRNCY_CD = '') ~> V164S8P1Derive",
				"V164S8P1Derive select(mapColumn(",
				"          WBS_ID,",
				"          COST_ELEM_CD,",
				"          BAS_PLAN_FINL_MO = MONTH_NUM,",
				"          BAS_PLAN_FINL_YR = YEAR_NUM,",
				"          OBJ_CRNCY_CD,",
				"          OBJ_AMT = WBS_ELEM_OC_AMT,",
				"          CONTRL_AMT = WBS_ELEM_CC_AMT,",
				"          CONTRL_CRNCY_CD = CNTRL_CRNCY_CD,",
				"          SOURCE_IND,",
				"          PLAN_AMT,",
				"          PLAN_CRNCY_CD,",
				"          TXN_AMT,",
				"          TXN_CRNCY_CD",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> V164S8P1",
				"V164S8P1, V164S8P2 join(V164S8P1@COST_ELEM_CD == V164S8P2@COST_ELEM_CD,",
				"     joinType:'inner',",
				"     matchType:'exact',",
				"     ignoreSpaces: false,",
				"     broadcast: 'auto')~> V164S14P2Join",
				"V164S14P2Join select(mapColumn(",
				"          WBS_ID,",
				"          COST_ELEM_CD = V164S8P1@COST_ELEM_CD,",
				"          VAL_CAT_CD,",
				"          BAS_PLAN_FINL_MO,",
				"          BAS_PLAN_FINL_YR,",
				"          OBJ_CRNCY_CD,",
				"          OBJ_AMT,",
				"          CONTRL_AMT,",
				"          CONTRL_CRNCY_CD,",
				"          PLAN_AMT,",
				"          PLAN_CRNCY_CD,",
				"          TXN_AMT,",
				"          TXN_CRNCY_CD,",
				"          SOURCE_IND",
				"     ),",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> V164S14P2",
				"V164S14P2 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     format: 'table',",
				"     preSQLs:[('DELETE FROM ' + $p_IPPF_RULES_SCHEME + '.WBS_BASE_CASE_PLAN_LOAD')],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError') ~> V164S14"
			]
		}
	}
}