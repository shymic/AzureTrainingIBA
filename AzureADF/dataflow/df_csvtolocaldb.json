{
	"name": "df_csvtolocaldb",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "CSV_Films_upd",
						"type": "DatasetReference"
					},
					"name": "NewData",
					"description": "Import data from CSV_Films_upd"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "FilterOutDeletes"
				},
				{
					"name": "CastColumns"
				},
				{
					"name": "AlterRowRules"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Column_1 as string,",
				"          Column_2 as string,",
				"          Column_3 as string,",
				"          Column_4 as string,",
				"          Column_5 as string,",
				"          Column_6 as string,",
				"          Column_7 as string,",
				"          Column_8 as string,",
				"          Column_9 as string,",
				"          Column_10 as string,",
				"          Column_11 as string",
				"     ),",
				"     allowSchemaDrift: false,",
				"     validateSchema: true,",
				"     ignoreNoFilesFound: false,",
				"     maxColumns: 15) ~> NewData",
				"NewData filter(SYS_CHANGE_OPERATION !='D') ~> FilterOutDeletes",
				"FilterOutDeletes cast(output(",
				"          Column_1 as string",
				"     ),",
				"     errors: true) ~> CastColumns",
				"CastColumns alterRow(insertIf(SYS_CHANGE_OPERATION=='I'),",
				"     deleteIf(SYS_CHANGE_OPERATION=='D'),",
				"     updateIf(SYS_CHANGE_OPERATION=='U')) ~> AlterRowRules"
			]
		}
	}
}