						"type": "Db2Source",
						"query": {
							"value": "SELECT\nCURCODE AS CRNCY_CD,\nUSD_TO_LOCAL_BS AS LCL_TO_US,\nLOCAL_TO_USD_BS AS US_TO_LCL\nFROM AIWCA.CURRENCY_RATE_R \nJOIN (SELECT \n         R.ACCTYR AS YEAR, R.ACCTMO AS MONTH\n         FROM AIWCA.CURRENCY_RATE_R R\n         ORDER BY R.ACCTYR DESC, R.ACCTMO DESC\n         FETCH FIRST 1 ROW ONLY\n      ) MAX_DATE \nON ACCTYR = YEAR\nAND ACCTMO = MONTH",
							"type": "Expression"
						}
					},
					"sink": {
						"type": "DelimitedTextSink",
						"storeSettings": {
							"type": "AzureBlobStorageWriteSettings"
						},
						"formatSettings": {
